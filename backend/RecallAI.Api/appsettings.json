{
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_POSTGRES_CONNECTION_STRING"
  },
  "Supabase": {
    "Url": "YOUR_SUPABASE_URL",
    "Key": "YOUR_SUPABASE_KEY",
    "JwtSecret": "YOUR_SUPABASE_JWT_SECRET"
  },
  "OpenAI": {
    "ApiKey": "",
    "TimeoutSeconds": 30,
    "MaxRetries": 3,
    "Models": {
      "Embeddings": "text-embedding-3-small",
      "MemoryEvaluation": "gpt-5-turbo",
      "HyDE": "gpt-5-turbo",
      "FinalResult": "gpt-5"
    }
  },
  "Completion": {
    "EnableMemorySearch": true,
    "MaxMemoryResults": 5,
    "MemoryThreshold": 0.7,
    "DefaultTemperature": 0.7,
    "DefaultMaxTokens": 1000
  },
  "EmbeddingCache": {
    "MaxSize": 1000,
    "ExpiryHours": 1
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information",
      "RecallAI.Api.Middleware.JwtValidationMiddleware": "Debug"
    }
  },
  "AllowedHosts": "*"
}